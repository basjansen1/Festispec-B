<Window x:Class="ProofOfConcept.Geodan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xaml="clr-namespace:Mapsui.UI.Wpf;assembly=Mapsui.UI.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <xaml:MapControl Name="MyMapControl">
        </xaml:MapControl>
        <TextBox x:Name="searchTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="168"/>
        <ComboBox x:Name="typeComboBox" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="85" SelectedIndex="0">
            <ComboBoxItem Content=""></ComboBoxItem>
            <ComboBoxItem Content="city"></ComboBoxItem>
            <ComboBoxItem Content="street"></ComboBoxItem>
        </ComboBox>
        <Button x:Name="searchButton" Content="Button" HorizontalAlignment="Left" Margin="103,38,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
