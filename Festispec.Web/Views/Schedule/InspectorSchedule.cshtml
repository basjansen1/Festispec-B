
@{
    ViewBag.Title = "InspectorSchedule";
}

<h2>InspectorSchedule</h2>

@model IEnumerable<Festispec.Domain.Schedule>

<div class="container">
    <div class="row">
        @foreach (var inspector in Model)
        {
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-3>@inspector.Id</div>
                    <li>@inspector.NotAvailableFrom</li>
                    <li>@inspector.NotAvailableTo</li>
                </div>
            </div>
        }
    </div>
</div>
